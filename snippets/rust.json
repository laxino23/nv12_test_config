{
  "Function": {
    "prefix": "fn",
    "body": [
      "fn ${1:name}(${2:arg}: ${3:Type}) -> ${4:RetType} {",
      "\t$0",
      "}"
    ],
    "description": "定义一个函数"
  },
  "Main Function": {
    "prefix": "main",
    "body": [
      "fn main() {",
      "\t$0",
      "}"
    ],
    "description": "main 主函数"
  },
  "Public Function": {
    "prefix": "pfn",
    "body": [
      "pub fn ${1:name}(${2:arg}: ${3:Type}) -> ${4:RetType} {",
      "\t$0",
      "}"
    ],
    "description": "定义一个公开函数"
  },
  "Struct": {
    "prefix": "struct",
    "body": [
      "#[derive(Debug, Clone)]",
      "pub struct ${1:Name} {",
      "\t${2:field}: ${3:Type},",
      "}"
    ],
    "description": "定义一个结构体"
  },
  "Struct with Implementation": {
    "prefix": "structimpl",
    "body": [
      "#[derive(Debug, Clone)]",
      "pub struct ${1:Name} {",
      "\t${2:field}: ${3:Type},",
      "}",
      "",
      "impl ${1:Name} {",
      "\tpub fn new(${2:field}: ${3:Type}) -> Self {",
      "\t\tSelf {",
      "\t\t\t${2:field},",
      "\t\t}",
      "\t}",
      "\t",
      "\t$0",
      "}"
    ],
    "description": "带有实现的结构体"
  },
  "Enum": {
    "prefix": "enum",
    "body": [
      "#[derive(Debug, Clone, PartialEq)]",
      "pub enum ${1:Name} {",
      "\t${2:Variant1},",
      "\t${3:Variant2}(${4:Type}),",
      "}"
    ],
    "description": "定义一个枚举"
  },
  "Impl Block": {
    "prefix": "impl",
    "body": [
      "impl ${1:Type} {",
      "\t$0",
      "}"
    ],
    "description": "实现块"
  },
  "Impl Trait": {
    "prefix": "implt",
    "body": [
      "impl ${1:Trait} for ${2:Type} {",
      "\t$0",
      "}"
    ],
    "description": "为类型实现 trait"
  },
  "Trait Definition": {
    "prefix": "trait",
    "body": [
      "pub trait ${1:Name} {",
      "\tfn ${2:method}(&self) -> ${3:RetType};",
      "\t$0",
      "}"
    ],
    "description": "定义一个 trait"
  },
  "Match Expression": {
    "prefix": "match",
    "body": [
      "match ${1:expr} {",
      "\t${2:pattern} => ${3:expr},",
      "\t_ => ${4:default},",
      "}"
    ],
    "description": "match 表达式"
  },
  "If Let": {
    "prefix": "ifl",
    "body": [
      "if let ${1:Some(value)} = ${2:expr} {",
      "\t$0",
      "}"
    ],
    "description": "if let 模式匹配"
  },
  "While Let": {
    "prefix": "whilel",
    "body": [
      "while let ${1:Some(value)} = ${2:expr} {",
      "\t$0",
      "}"
    ],
    "description": "while let 循环"
  },
  "For Loop": {
    "prefix": "for",
    "body": [
      "for ${1:item} in ${2:iterator} {",
      "\t$0",
      "}"
    ],
    "description": "for 循环"
  },
  "Loop": {
    "prefix": "loop",
    "body": [
      "loop {",
      "\t$0",
      "}"
    ],
    "description": "无限循环"
  },
  "While Loop": {
    "prefix": "while",
    "body": [
      "while ${1:condition} {",
      "\t$0",
      "}"
    ],
    "description": "while 循环"
  },
  "Let Binding": {
    "prefix": "let",
    "body": [
      "let ${1:name} = ${2:value};"
    ],
    "description": "let 绑定"
  },
  "Mutable Let": {
    "prefix": "letm",
    "body": [
      "let mut ${1:name} = ${2:value};"
    ],
    "description": "可变 let 绑定"
  },
  "Result": {
    "prefix": "result",
    "body": [
      "Result<${1:T}, ${2:E}>"
    ],
    "description": "Result 类型"
  },
  "Option": {
    "prefix": "option",
    "body": [
      "Option<${1:T}>"
    ],
    "description": "Option 类型"
  },
  "Ok": {
    "prefix": "ok",
    "body": [
      "Ok(${1:value})"
    ],
    "description": "Ok 变体"
  },
  "Err": {
    "prefix": "err",
    "body": [
      "Err(${1:error})"
    ],
    "description": "Err 变体"
  },
  "Some": {
    "prefix": "some",
    "body": [
      "Some(${1:value})"
    ],
    "description": "Some 变体"
  },
  "Println": {
    "prefix": "println",
    "body": [
      "println!(\"${1:{}}\", ${2:value});"
    ],
    "description": "打印到控制台"
  },
  "Print": {
    "prefix": "print",
    "body": [
      "print!(\"${1:{}}\", ${2:value});"
    ],
    "description": "打印到控制台(不换行)"
  },
  "Debug Print": {
    "prefix": "dbg",
    "body": [
      "dbg!(${1:value});"
    ],
    "description": "调试打印"
  },
  "Vec": {
    "prefix": "vec",
    "body": [
      "vec![${1:elements}]"
    ],
    "description": "创建 Vec"
  },
  "String": {
    "prefix": "string",
    "body": [
      "String::from(\"${1:text}\")"
    ],
    "description": "创建 String"
  },
  "Use": {
    "prefix": "use",
    "body": [
      "use ${1:module}::${2:item};"
    ],
    "description": "导入模块"
  },
  "Mod": {
    "prefix": "mod",
    "body": [
      "mod ${1:name};"
    ],
    "description": "模块声明"
  },
  "Test Function": {
    "prefix": "test",
    "body": [
      "#[test]",
      "fn ${1:test_name}() {",
      "\t$0",
      "}"
    ],
    "description": "测试函数"
  },
  "Test Module": {
    "prefix": "testmod",
    "body": [
      "#[cfg(test)]",
      "mod tests {",
      "\tuse super::*;",
      "",
      "\t#[test]",
      "\tfn ${1:test_name}() {",
      "\t\t$0",
      "\t}",
      "}"
    ],
    "description": "测试模块"
  },
  "Assert": {
    "prefix": "assert",
    "body": [
      "assert!(${1:condition});"
    ],
    "description": "断言"
  },
  "Assert Eq": {
    "prefix": "asserteq",
    "body": [
      "assert_eq!(${1:left}, ${2:right});"
    ],
    "description": "相等断言"
  },
  "Derive": {
    "prefix": "derive",
    "body": [
      "#[derive(${1:Debug, Clone})]"
    ],
    "description": "派生宏"
  },
  "Allow": {
    "prefix": "allow",
    "body": [
      "#[allow(${1:lint})]"
    ],
    "description": "允许特定 lint"
  },
  "Cfg": {
    "prefix": "cfg",
    "body": [
      "#[cfg(${1:condition})]"
    ],
    "description": "条件编译"
  },
  "Async Function": {
    "prefix": "asyncfn",
    "body": [
      "async fn ${1:name}(${2:arg}: ${3:Type}) -> ${4:RetType} {",
      "\t$0",
      "}"
    ],
    "description": "异步函数"
  },
  "Tokio Main": {
    "prefix": "tokiomain",
    "body": [
      "#[tokio::main]",
      "async fn main() {",
      "\t$0",
      "}"
    ],
    "description": "Tokio 异步 main 函数"
  },
  "Unwrap Or": {
    "prefix": "unwrapor",
    "body": [
      "unwrap_or(${1:default})"
    ],
    "description": "unwrap_or 方法"
  },
  "Map": {
    "prefix": "map",
    "body": [
      "map(|${1:x}| ${2:x})"
    ],
    "description": "map 方法"
  },
  "Filter": {
    "prefix": "filter",
    "body": [
      "filter(|${1:x}| ${2:condition})"
    ],
    "description": "filter 方法"
  },
  "Closure": {
    "prefix": "closure",
    "body": [
      "|${1:args}| ${2:body}"
    ],
    "description": "闭包"
  },
  "Question Mark": {
    "prefix": "?",
    "body": [
      "${1:expr}?"
    ],
    "description": "? 操作符"
  },
  "Box New": {
    "prefix": "box",
    "body": [
      "Box::new(${1:value})"
    ],
    "description": "创建 Box"
  },
  "Rc New": {
    "prefix": "rc",
    "body": [
      "Rc::new(${1:value})"
    ],
    "description": "创建 Rc"
  },
  "Arc New": {
    "prefix": "arc",
    "body": [
      "Arc::new(${1:value})"
    ],
    "description": "创建 Arc"
  }
}
